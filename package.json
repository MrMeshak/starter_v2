  "scripts": {
    "build": "rimraf dist && swc ./src -d dist",
    "start": "node dist/index.js",
    "start:docker": "pnpm build && node --inspect=0.0.0.0 dist/index.js",
    "db:console": "docker compose run postgres psql -h localhost -U postgres -d webaap_dev",
    "db:migrate": "pnpx prisma db push",
    "db:migrate:docker": "docker compose run backend pnpm db:migrate",
    "rebuild:be": "docker compose build backend && docker compose rm --force --stop backend && docker compose up -d backend"
  },
